        <item name="colorPrimary">@color/yellow_primary</item>
        <item name="colorPrimaryVariant">@color/yellow_primary_dark</item>
        <item name="colorOnPrimary">@color/yellow_on_primary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow_primary</item>
        <item name="colorSecondaryVariant">@color/yellow_primary_dark</item>
